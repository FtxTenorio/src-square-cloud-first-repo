NODE_ENV=development
SERVER_PORT=3000
# Origens permitidas para CORS (separadas por vírgula). Em dev, localhost:3000 é permitido por padrão.
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
DISCORD_SECRET_KEY=exemplo_discord_secret
REDIS_CA_CERT=exemplo_redis_ca_cert
REDIS_URL=rediss://usuario:senha@host:port
MONGO_URI=mongodb://usuario:senha@host:port
MONGO_CA_CERT=exemplo_mongo_ca_cert
MONGO_CLIENT_CERT=exemplo_mongo_client_cert
OPENAI_API_KEY=exemplo_openai_api_key

# EventBridge Scheduler (Life-Sync / rotinas) – ver docs/EVENTBRIDGE.md
AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=opcional_se_usar_credential_chain
# AWS_SECRET_ACCESS_KEY=opcional_se_usar_credential_chain
EVENTBRIDGE_LAMBDA_ARN=arn:aws:lambda:us-east-1:ACCOUNT:function:routine-trigger
EVENTBRIDGE_SCHEDULER_ROLE_ARN=arn:aws:iam::ACCOUNT:role/eventbridge-scheduler-routine-role
EVENTBRIDGE_SCHEDULE_GROUP=life-sync